<!DOCTYPE html>
<html>
<head>
  <title>CSE2017 web | <%= @page_title %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="header">
  <h1>CSE2017 Project</h1>
</div>
<div id="main">
  <% if !flash[:notice].blank? %>
      <div class="notice">
        <%= flash[:notice] %>
      </div>
  <% end %>
  <div id="content">
    <%= yield %>
  </div>
</div>
</body>
</html>
